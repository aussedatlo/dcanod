{
  "name": "dcanod",
  "version": "1.0.0",
  "description": "",
  "author": "Louis Aussedat",
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "build": "rimraf bin/* && tsc -p . && npm run copy-files",
    "build:watch": "rimraf bin/* && tsc --watch -p . && npm run copy-files",
    "prettier": "prettier --write 'src/**/*.ts'",
    "copy-files": "copyfiles -u 1 src/**/*.ejs lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aussedatlo/dcanod"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "cli",
    "node",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/aussedatlo/dcanod/issues"
  },
  "homepage": "https://github.com/aussedatlo/dcanod#readme",
  "bin": {
    "dcanod-cli": "bin/index.js"
  },
  "dependencies": {
    "binance-api-node": "^0.12.0",
    "chalk": "^4.1.0",
    "chart.js": "^3.9.1",
    "commander": "^7.0.0",
    "console-table-printer": "^2.8.2",
    "ejs": "^3.1.8",
    "figlet": "^1.5.0",
    "html-tableify": "^0.0.2",
    "nexo-pro": "^0.1.1",
    "prompts": "^2.4.1",
    "sqlite3": "^5.1.2",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/figlet": "^1.2.1",
    "@types/node": "^14.14.21",
    "copyfiles": "^2.4.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier"
    ]
  }
}
